nterms = int(input("Number of terms required:"))
i, j = 0, 1
count = 0
if nterms <= 0:
   print("Enter a positive integer")
elif nterms == 1:
   print("Fibonacci sequence upto",nterms,":")
   print(i)
else:
   print("Fibonacci sequence:")
   while count < nterms:
       print(i)
       k = i + j
       i = j
       j = k
       count += 1
